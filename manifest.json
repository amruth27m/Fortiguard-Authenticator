{
  "manifest_version": 2,

  "name": "NIT Calicut Authenticator",
  "description": "This extension automatically logs you into NITC network",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "https://ajax.googleapis.com/",
	"storage"
  ]
}
